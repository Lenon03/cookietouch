{
  "name": "cookietouch",
  "version": "0.0.1",
  "description": "DofusTouch Bot with Electron React/TypeScript",
  "main": "dist/main/main.js",
  "scripts": {
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir",
    "dev": "electron-webpack dev",
    "lint": "tslint -c tslint.json 'src/**/*.tsx' 'src/**/*.ts' -t stylish",
    "lint:fix": "tslint -c tslint.json 'src/**/*.tsx' 'src/**/*.ts' -t stylish --fix",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "postinstall": "electron-builder install-app-deps",
    "release": "yarn compile && build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cookie-project/cookietouch.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "webpack",
    "cookie",
    "cookietouch"
  ],
  "author": "Christopher Yovanovitch <yovano_c@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cookie-project/cookietouch/issues"
  },
  "homepage": "https://github.com/cookie-project/cookietouch#readme",
  "devDependencies": {
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.18",
    "@types/react-dom": "^16.0.5",
    "awesome-typescript-loader": "^5.0.0",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^2.0.2",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.2",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-loader": "^3.6.0",
    "webpack": "^4.10.2",
    "webpack-build-notifier": "^0.1.27"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "electron-log": "^2.2.15",
    "electron-updater": "^2.21.10",
    "firebase": "^5.0.4",
    "linqts": "^1.11.0",
    "lodash": "^4.17.10",
    "material-design-icons": "^3.0.1",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "react": "^16.4.1",
    "react-color": "^2.14.1",
    "react-dom": "^16.4.1",
    "source-map-support": "^0.5.6",
    "typeface-roboto": "^0.0.54",
    "typescript": "2.9.2"
  }
}
